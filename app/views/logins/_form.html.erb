<%= form_for :logins do |f| %>
  <p>Email:<br> <%= f.email_field :email %>
  <%= errors_for(f, :email) %></p>
  <p>Password:<br> <%= f.password_field :password %>
  <%= errors_for(f, :password) %></p>
  <%= f.submit 'Log in', 
      class: "hover:bg-teal-500 text-gray-700 font-semibold hover:text-white py-2 px-4 border border-gray-500 rounded my-3" %> 
<% end %>